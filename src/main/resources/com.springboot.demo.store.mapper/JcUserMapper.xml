<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.demo.store.mapper.JcUserMapper">

    <select id="getMax" parameterType="Integer" resultType="java.lang.Integer">
       SELECT max(age) FROM jc_user where id=#{userId}
    </select>

</mapper>
